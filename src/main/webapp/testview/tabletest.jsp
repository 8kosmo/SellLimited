function total_search(){
	var search1 = $("#search_sel1").val();
	var search2 = $("#search_sel2").val();
	var search_target = search1.split('_');
	if(search_target[1]){
		var val3 = search_target[1];
		if(val3=='amazon') val3 = 'amazon_jp';
	}else{
		var val3 = search1;
	}
	if(search2 == 'select_all'){
		var val4 = '';
	}else{
		var val4 = search2;
	}

	var total_keyword = encodeURIComponent($("#SearchWord").val());

	if(total_keyword == ''){
		alert('검색할 상품명이나 브랜드를 입력 해 주세요.');
		$("#SearchWord").focus();
		return false;
	}

	if(val3 == '' || val3 == 'undefined'){
		location.href='/hey/search?keyword='+total_keyword+'&type=buy';
		return false;
	}else{
		if(val3 == 'yhauc'){ // 야후경매
			if(val4 == 'auction_id' || ((val4 == '' || val4 == 'undefined') && total_keyword.length=='10' && chkType(total_keyword, 'falnum')===true)){
				location.href='/deal/detail?url=https://page.auctions.yahoo.co.jp/jp/auction/'+total_keyword+'&shop_id=yhauc&lo=';
			}else if(val4 == 'seller_id'){
				location.href='/auction/buy?shop_id=yhauc&seller='+total_keyword;
			}else{
				location.href='/auction/buy?level1=&shop_id=yhauc&keyword='+total_keyword+'&type4='+val4;
			}
			return false;
		}else{
			if(val4 == 'goods_id'){
				if(val3=='rakuten'){
					var total_keyword_replace = total_keyword.replace('%3A','/');
					location.href='/deal/detail?url=https://item.rakuten.co.jp/'+total_keyword_replace+'/&shop_id=rakuten&lo=';
				}
				if(val3=='amazon_jp') location.href='/deal/detail?url='+total_keyword+'&shop_id='+val3+'&lo=';
				if(val3=='mercari') location.href='/deal/detail?url=https://item.mercari.com/jp/'+total_keyword+'/&shop_id=mercari&lo=';
				return false;
			}
			if(val4 == 'seller_id'){
				if(val3=='ebay_buy_motors') val3 = 'ebay_buy';
				location.href='/category/buy?shop_id='+val3+'&seller='+total_keyword;
				return false;
			}
			location.href='/category/buy?level1=&shop_id='+val3+'&keyword='+total_keyword+'&type4='+val4;
			return false;
		}
	}
}


