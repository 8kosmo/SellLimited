// 휴대폰 인증
function phone_auth_prc(id)
{
	var param = {};
	var url = '';
	var $jct = $('input[name=jct]');
	var $realname = $('input[name=realname]');
	var $mobile = $('input[name=mobile]');
	var $csrf_token = $('input[name=csrf_token]');

	var $verify_key = $('input[name=verify_key]');
	var $verify_number = $('input[name=verify_number]');
	var $notice = '';
	$('#notice_name').html('');

	$mobile_auth = $('input[name=mobile_auth]');
	if($mobile_auth.val() == '') {
		$('#mobile_notice').html('<div style="color:#fd5c03">※ 휴대폰 번호를 입력하셔야 합니다.</div>');
		$mobile_auth.focus();
		return false;
	}


	if(id == 'req'){
		var jbExcerpt = $mobile_auth.val().substring( 0, 2 );
		if(jbExcerpt != "01"){
			alert('휴대폰 만 인증 가능 합니다.');
			return false;
		}
	}


	switch(id) {
		case 'authreq' :
			if ( ! $verify_key.val()) {
				alert('인증번호받기를 먼저 진행해주세요');
				return false;
			}

			url = '/auth/send_confirm';
			param = { "csrf_token" : $csrf_token.val(), "jct" : $jct.val(), "mobile" : $mobile_auth.val(), "verify_key" : $verify_key.val(), "verify_number" : $verify_number.val() }
			break;
		default :

			if ( ! $mobile_auth.val() ) {
				$mobile_auth.focus();
				return false;
			}

			url = '/auth/sendsms';
			param = { "csrf_token" : $csrf_token.val(), "jct" : $jct.val(), "mobile" : $mobile_auth.val() }
			break;
	}


	$.post(url, param, function(d){
		if (id == 'req') {
			switch (d.result) {
				case 'succuess' :
					$jct.val(d.jct);
					$verify_key.val(d.verify_key);
					$('#mobile_notice').html('<span style="color:#929292;">※ 인증번호를 발송하였습니다.</span>');
					$verify_number.focus();
					break;
				case 'failed' :
					if (d.message == 'Too many request') d.message = '너무 잦은 요청이니 잠시후 시도하세요..';
					if (d.message == '<p>이미 등록된 휴대폰입니다.</p>') d.message = '사용중인 휴대폰번호입니다.';
					$('#mobile_notice').html('<span style="color:#fd5c03">※ '+d.message+'</span>');
					break;
			}
		} else if (id == 'authreq') {
			switch (d.result) {
				case 'succuess' :
					$mobile.val($mobile_auth.val());
					$(".auth_form").hide();
					$('#mobile_notice').html('<span style="color:#929292;">※ 성공했습니다. 가입을 계속 진행해주세요</span>');
					break;
				case 'failed' :
					if(d.message == 'Fail to verify the number') d.message = '인증번호를 다시 확인해 주세요.';
					alert(d.message);
					break;
			}
		}
	}, 'json');

}


function showKeyCode(event) {
	event = event || window.event;
	var keyID = (event.which) ? event.which : event.keyCode;
	if( ( keyID >=48 && keyID <= 57 ) || ( keyID >=96 && keyID <= 105 ) || keyID == 8 || keyID == 46 || keyID == 37 || keyID == 39 )
	{
		return true;
	}
	else
	{
		return false;
	}
}

