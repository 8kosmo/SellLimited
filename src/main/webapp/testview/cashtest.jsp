	$('input[name=price]').focus();
}

function cash_zero()
{
	if($(":input:radio[name=pay_method]:checked").val() == 2 || $(":input:radio[name=pay_method]:checked").val() == 3 ) {

		alert('카드 결제시 수동 입력 하실수 없습니다.');
		return false;

	} else {

		$('input[name=price]').val('');
		$('input[name=price]').focus();

	}
}

// 결제 수단 선택 표시
$('input[name=pay_method]').click(function() {
	
	var chk_type = $(this).val();

	var credit_method = $('#credit_method').val();
	if(credit_method == 2) {

		if (chk_type == 4) {
			$('input[name=price]').val('');
			$('input[name=price]').attr("readonly", false);
		}else{
			$('input[name=price]').val('');
			$('input[name=price]').attr("readonly", true);
		}


	}else{

		if(chk_type == 4) {

			$('input[name=price]').val('');
			$('input[name=price]').attr("readonly",false);
			$('.select_card').hide();
			$('.select_bank').show();

		} else if(chk_type == 2) {

			$('input[name=price]').val('');
			$('input[name=price]').attr("readonly",true);
			$('.select_bank').hide();
			$('.select_card').show();

		} else if(chk_type == 3) {

			$('input[name=price]').val('');
