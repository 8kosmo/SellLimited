			$('.select_card').hide();
			$('.select_bank').show();
		}

	}


});

$('#order_frm').submit(function(){
	execPay($(this));
	return false;
});

function execPay($form)
{
	var $pay_method = $('input[name=pay_method]:checked');
	var price = $('input[name=price]').val();

	
//	if ($pay_method.val() == 4) {
//		if( ! $('input[name=deposit_name]').val()) {
//			alert("입금자명을 입력하세요.");
//			$('input[name=deposit_name]').focus();
//			return false;
//		}
//	}

	if(price <= 0) {
		alert('충전 금액을 선택하셔야 합니다.');
		$('input[name=price]').focus();
		return false;
	}

	if ($pay_method.val() == 1)
		//popup = window.open("", "payment_popup", "width=500,height=680,scroll=no,help=no,toolbar=no,resizable=no,status=yes");
	if ($pay_method.val() == 2 || $pay_method.val() == 3 )	// 다날 휴대폰, 신용카드, 계좌이체)
		popup = window.open("", "payment_popup", "width=520,height=380,scroll=no,help=no,toolbar=no,resizable=no,status=yes");
	

	var credit_method = $('#credit_method').val();
	if(credit_method != 2) {

		if($(':radio[name="pay_method"]:checked').val() == 2){
			if($('#LGD_CARDTYPE').val() == 'XX' || $('#LGD_CARDTYPE').val() == ''){
				alert('결제하실 카드종류를 선택 해 주세요.');
				return false;
			}
		}else{
			if($('#LGD_BANKCODE').val() == 'XX' || $('#LGD_BANKCODE').val() == ''){
				alert('입금은행을 선택 해 주세요.');
				return false;
			}
			if($('#LGD_ACCOUNTOWNER').val() == ''){
				alert('입금자명을 입력 해 주세요.');
				return false;
			}
		}
	}


	$.ajax({
		url: '/pay/buy_cash',
		type: 'POST',
		data: $form.serialize(),
		dataType: 'json'
	}).done(function(data){
		switch(data.result) {
			case 'success' :

				showPopup();

				$('input[name=cash_id]').val(data.id);
				$('input[name=cash_price').val(price);
//				$('input[name=pay_method').val($pay_method.val());
				var $danal_form = $('#danal_frm');
				$danal_form.find('input[name=order_id]').val(data.order_id);
				$danal_form.find('input[name=pay_method]').val($pay_method.val());
				$danal_form.submit();
			break;
			case 'done' :
				alert('무통장 신청이 완료 되었습니다.');
				document.location.href='/mypage/latest';
				break;
			case 'login' :
				alert(data.message);
				goLogin();
			break;
			case 'item' :
			case 'soldout' :
				alert(data.message);
				if (popup) popup.close();
				location.href = "/";
			break;
			case 'stock' :
			case 'stock_over' :
			case 'purcharse_max' :
				alert(data.message);
				isBuy = false;
			break;
			case 'point' :
			case 'min_amount' :
				alert(data.message);
				isBuy = false;
			break;
			default:
				alert(data.message);
				isBuy = false;
				location.reload();	//20170922
			break;
		}
	});
}
