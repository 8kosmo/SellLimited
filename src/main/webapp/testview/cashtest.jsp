	}else if(LGD_CARDTYPE == '33'){
		$("#payment_frame").css('width' , '700');
		$("#payment_frame").css('height' , '600');
	}else{
		$("#payment_frame").css('width' , '490');
		$("#payment_frame").css('height' , '530');
	}
	$("#payment_frame").show();
	$("#payment_frame").center();
}


function cash_input_add(price)
{
